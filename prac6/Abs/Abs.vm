// Calculates y = |x|
// x & y are static variables

push constant 256
pop sp
push constant 300
pop local 0
push constant 400
pop argument 0
push constant 3000
pop this 0
push constant 3010
pop that 0
// Load x onto the stack
push static 0
// Check if x is negative
neg
if-goto NEGATIVE
// x is non-negative, so set y to x
push static 0
goto END
// x is negative, so set y to -x
label NEGATIVE
push static 0
neg

label END
goto END