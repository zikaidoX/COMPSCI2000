// Calculates y = |x|
// x & y are static variables

// Check if x is negative
push static 0  // push x onto the stack
lt            // check if x < 0
if-goto NEGATIVE  // if x < 0, go to NEGATIVE

// x is positive, leave it as it is
goto END

// x is negative, multiply it by -1
label NEGATIVE
push constant -1  // push -1 onto the stack
neg               // multiply x by -1

// Store the absolute value in y
pop static 1  // pop the absolute value from the stack and store it in y

// End of the program
label END