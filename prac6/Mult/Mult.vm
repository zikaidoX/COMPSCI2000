// Calculates a = x*y
// a is a local variable
// x & y are static variables

// Initialize a to 0
push constant 0
pop local 0

// Input value for x
push static 0
pop argument 0

// Input value for y
push static 1
pop argument 1

// Loop to multiply x and y
(INIT_LOOP)
  // Check if y is 0 (base case)
  push argument 1
  if-goto END // Jump to END if y is 0

  // Add x to a
  push local 0
  push argument 0
  add
  pop local 0

  // Decrement y
  push argument 1
  push constant 1
  sub
  pop argument 1

  // Repeat the loop
  goto INIT_LOOP


label END
goto END