// Calculates a = x*y
// a is a local variable
// x & y are static variables

// Set up local variables
push constant 0
pop local 0
push constant 0
pop local 1
push constant 0
pop local 2

// Load x and y into the stack
push static 0
push static 1

// Set up loop counter
push constant 0
pop local 3

// Loop to calculate x * y
(MULT_LOOP)
  // Check if the loop counter is equal to y
  push local 3
  push static 1
  eq
  if-goto END_MULT_LOOP

  // Add x to the result
  push local 2
  push static 0
  add
  pop local 2

  // Increment the loop counter
  push local 3
  push constant 1
  add
  pop local 3

  goto MULT_LOOP

// Set the result to the local variable a
(END_MULT_LOOP)
push local 2
pop local 0

// End of multiplication algorithm
label END
goto END